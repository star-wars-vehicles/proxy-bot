{
  "name": "proxy-bot",
  "private": true,
  "version": "0.1.0",
  "description": "The Discord bot server the Star Wars Vehicles Discord server.",
  "main": "build/index.js",
  "scripts": {
    "build": "gulp build",
    "lint": "eslint src/**/* && tsc --noEmit",
    "start": "gulp start"
  },
  "keywords": [
    "discord",
    "star",
    "wars",
    "discordjs",
    "discord",
    "bot"
  ],
  "author": "David Katz",
  "license": "GPL-3.0",
  "devDependencies": {
    "@types/gulp": "^4.0.6",
    "@types/gulp-nodemon": "0.0.32",
    "@types/gulp-sourcemaps": "0.0.32",
    "@types/mongoose": "^5.5.8",
    "@types/node": "^12.6.2",
    "@types/node-fetch": "^2.3.7",
    "@typescript-eslint/eslint-plugin": "^1.12.0",
    "@typescript-eslint/parser": "^1.12.0",
    "del": "^5.0.0",
    "eslint": "^5.16.0",
    "gulp": "^4.0.2",
    "gulp-nodemon": "^2.4.2",
    "gulp-sourcemaps": "^2.6.5",
    "gulp-ts-alias": "^0.2.0",
    "gulp-typescript": "^5.0.1",
    "nodemon": "^1.19.1",
    "ts-node": "^8.3.0",
    "typescript": "^3.5.3"
  },
  "dependencies": {
    "discord-paginationembed": "^1.0.0-beta.4",
    "discord.js": "github:discordjs/discord.js",
    "discord.js-commando": "github:discordjs/commando",
    "mongoose": "^5.6.4",
    "mongoose-autopopulate": "^0.9.1",
    "mongoose-findorcreate": "^3.0.0",
    "node-fetch": "^2.6.0",
    "reflect-metadata": "^0.1.13",
    "sqlite": "^3.0.3",
    "steamapi": "^2.0.7",
    "typegoose": "^5.7.2"
  },
  "optionalDependencies": {
    "bufferutil": "^4.0.1"
  },
  "eslintConfig": {
    "env": {
      "node": true,
      "es6": true,
      "browser": true,
      "jest": true
    },
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "project": "./tsconfig.json"
    },
    "plugins": [
      "@typescript-eslint"
    ],
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended"
    ],
    "rules": {
      "semi": [
        "error",
        "always"
      ],
      "no-console": "off",
      "no-dupe-class-members": "off",
      "@typescript-eslint/indent": [
        "error",
        4
      ]
    }
  }
}
